.tools-bar {
  display flex
  align-items center
  justify-content space-between
  width 100%
  height 100%

  .left {
    .dir-info {
      margin-left 2rem

      .el-icon {
        margin-right 4rem
        color var(--text-color-3)
        font-size 16rem
      }
    }
  }


  .right {
    .el-icon {
      margin-left 10rem
      color var(--text-color-3)
      font-size 22rem
      cursor pointer
    }
  }
}
